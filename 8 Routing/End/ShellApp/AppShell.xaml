<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ShellApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:ShellApp.Pages"
    Shell.FlyoutBehavior="Flyout"
    Title="ShellApp">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate pages:HomePage}"
        Route="Home" />
    <ShellContent
        Title="Testimonials"
        ContentTemplate="{DataTemplate pages:TestimonialDetailPage}"
        Route="Testimonaials" />
    <ShellContent
        Title="Community"
        ContentTemplate="{DataTemplate pages:CommunityListPage}"
        Route="Community" />
    <MenuItem Clicked="MenuItem_Clicked" Text="About" />

</Shell>
